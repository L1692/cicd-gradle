pipeline {

    agent any

    stages {
        stage('Build') {
            steps {
                jacoco()
                gradle build sonarqube -Dsonar.host.url=$SONAR_HOST_URL
            }
        }
        stage('Test') {
            steps {
                echo 'Testing......'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}